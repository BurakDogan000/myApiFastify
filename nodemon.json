{
    "restartable": "rs",
    "ignore": [".git", "node_modules/", "dist/", "coverage/"],
    "watch": ["src/", "config/"],
    "execMap": {
      "ts": "node --trace-warnings -r ts-node/register -r tsconfig-paths/register"
    },
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js, ts"
  }